{
	"name": "modernizacao-manejo",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate-client": "rm -rf src/client && openapi --input https://api.henriquesebastiao.com/openapi.json --output ./src/client --client axios"
	},
	"devDependencies": {
		"@playwright/test": "^1.35.1",
		"@skeletonlabs/skeleton": "^1.8.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"@vite-pwa/sveltekit": "^0.2.5",
		"autoprefixer": "^10.4.14",
		"axios": "^1.4.0",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.31.0",
		"form-data": "^4.0.0",
		"openapi-typescript-codegen": "^0.24.0",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"kolorist": "^1.8.0",
		"svelte-frappe-charts": "^1.10.0",
		"svelte-icons": "^2.1.0"
	}
}
